{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@styles/*": ["src/styles/*"],
      "@assets/*": ["src/assets/*"],
      "@constants/*": ["src/constants/*"],
      "@common/*": ["src/modules/common/*"],
      "@home/*": ["src/modules/home/*"],
      "@projects/*": ["src/modules/projects/*"],
      "@promo/*": ["src/modules/promo/*"],
      "@wedding/*": ["src/modules/wedding/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
